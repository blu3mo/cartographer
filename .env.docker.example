# =============================================================================
# Cartographer Docker Compose Environment Configuration
# =============================================================================
# Copy this file to .env.docker and fill in your values
# This file is used by docker-compose.yml

# -----------------------------------------------------------------------------
# Database Configuration
# -----------------------------------------------------------------------------
POSTGRES_PASSWORD=postgres

# -----------------------------------------------------------------------------
# Supabase Configuration
# -----------------------------------------------------------------------------
# JWT Secret for Supabase (generate with: openssl rand -base64 32)
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

# Supabase API Keys (these are demo keys, you can use them for local development)
ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24iLCJleHAiOjE5ODM4MTI5OTZ9.CRXP1A7WOeoJeXxjNni43kdQwgnWNReilDMblYTn_I0
SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU

# Supabase URL (for local development)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:8000
SUPABASE_PUBLIC_URL=http://localhost:8000

# -----------------------------------------------------------------------------
# LLM Provider Configuration
# -----------------------------------------------------------------------------
# Choose ONE of the following options:

# Option 1: OpenRouter (default)
# Get your API key from https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here
USE_VERTEX_AI=false

# Option 2: Google Vertex AI
# Uncomment and configure the following if you want to use Vertex AI instead
# USE_VERTEX_AI=true
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id
# GOOGLE_APPLICATION_CREDENTIALS=/app/credentials/vertex-ai-key.json
# GOOGLE_APPLICATION_CREDENTIALS_PATH=/path/to/your/vertex-ai-key.json

# -----------------------------------------------------------------------------
# Optional: Access Control
# -----------------------------------------------------------------------------
# Uncomment to enable Basic Authentication
# BASIC_AUTH_USERNAME=admin
# BASIC_AUTH_PASSWORD=secret

# Uncomment to enable IP allowlist (comma-separated)
# ALLOWED_IPS=203.0.113.10,198.51.100.7
